<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="30dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0">
            <shape android:shape="rectangle" >
                <solid android:color="@color/colorWhite" />
                <stroke android:color="@color/colorDino"
                    android:width="1dp"/>
            </shape>
        </rotate>
    </item>
    <item android:top="13dp"
        android:right="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorWhite" />
            <corners android:radius="8dp" />
            <stroke android:width="1dp"
                android:color="@color/colorDino"/>
        </shape>
    </item>
    <item
        android:left="18dp"
        android:top="13dp"
        android:right="369dp"
        android:bottom="400dp">
            <shape android:shape="rectangle">
                <solid android:color="@color/colorWhite" />
            </shape>
   </item>
</layer-list>
